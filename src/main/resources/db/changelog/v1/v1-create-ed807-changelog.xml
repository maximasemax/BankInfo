<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
        xmlns:pro="http://www.liquibase.org/xml/ns/pro"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog-ext
    http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd
    http://www.liquibase.org/xml/ns/pro
    http://www.liquibase.org/xml/ns/pro/liquibase-pro-4.0.xsd
    http://www.liquibase.org/xml/ns/dbchangelog
    http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.0.xsd
    ">

    <changeSet id="v1-1" author="Maxim Yarmol">
        <createTable tableName="ed807">
            <column name="id" type="bigint" autoIncrement="true">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="creation_reason" type="varchar(255)">
                <constraints nullable="true"/>
            </column>
            <column name="creation_date_time" type="timestamp">
                <constraints nullable="true"/>
            </column>
            <column name="info_type_code" type="varchar(255)">
                <constraints nullable="true"/>
            </column>
            <column name="business_day" type="varchar(255)">
                <constraints nullable="true"/>
            </column>
            <column name="directory_version" type="bigint">
                <constraints nullable="true"/>
            </column>
            <column name="ed_no" type="bigint">
                <constraints nullable="true"/>
            </column>
            <column name="ed_date" type="date">
                <constraints nullable="true"/>
            </column>
            <column name="ed_author" type="varchar(255)">
                <constraints nullable="true"/>
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>